#!/usr/bin/env ruby
# Add '.rb' to work around a bug in IronRuby's File#dirname
$:.unshift(File.dirname(__FILE__ + '.rb') + '/../lib') unless $:.include?(File.dirname(__FILE__ + '.rb') + '/../lib')

begin 
  require 'common'
rescue LoadError
  require 'bin/common'
end

require 'cucumber/cli'
Cucumber::CLI.execute(ARGV)
